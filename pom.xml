<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
 <modelVersion>4.0.0</modelVersion>
 <groupId>org.freebus.fts</groupId>
 <artifactId>parent</artifactId>
 <version>0.1-SNAPSHOT</version>
 <name>freebus-fts-parent</name>
 <packaging>pom</packaging>
 <organization>
  <name>The Freebus Project</name>
  <url>http://www.freebus.org</url>
 </organization>
 <developers>
  <developer>
   <id>stefant</id>
   <name>Stefan Taferner</name>
   <email>stefan.taferner@gmx.at</email>
   <organization>The Freebus Project</organization>
   <organizationUrl>http://www.freebus.org</organizationUrl>
   <timezone>1</timezone>
  </developer>
 </developers>
 <scm>
  <connection>scm:svn:svn+ssh://svn.freebus.org/usr/local/svn/freebus/trunk/software_fts</connection>
  <developerConnection>scm:svn:svn+ssh://svn.freebus.org/usr/local/svn/freebus/trunk/software_fts</developerConnection>
 </scm>
 <modules>
  <module>freebus-fts-knxcomm</module>
  <module>freebus-fts-persistence</module>
  <module>freebus-fts-products</module>
  <module>freebus-fts-project</module>
  <module>freebus-fts</module>
 </modules>
 <distributionManagement>
  <repository>
   <uniqueVersion>false</uniqueVersion>
   <id>stefant-webspace</id>
   <name>Stefan's Webspace</name>
   <url>ftp://stefan.members.xpirio.at/htdocs/fts</url>
  </repository>
 </distributionManagement>
 <dependencies>
  <dependency>
   <groupId>junit</groupId>
   <artifactId>junit</artifactId>
   <version>4.7</version>
   <scope>test</scope>
  </dependency>
  <dependency>
  	<groupId>apache-log4j</groupId>
  	<artifactId>log4j</artifactId>
  	<version>1.2.15</version>
  </dependency>
 </dependencies>
 <build>
  <plugins>
   <plugin>
    <artifactId>maven-compiler-plugin</artifactId>
    <configuration>
     <source>1.6</source>
     <target>1.6</target>
    </configuration>
   </plugin>
  </plugins>
 </build>
 <repositories>
  <repository>
   <id>org.eclipse.persistence</id>
   <name>eclipselink-repo</name>
   <url>http://www.eclipse.org/downloads/download.php?r=1&amp;nf=1&amp;file=/rt/eclipselink/maven.repo</url>
  </repository>
  <repository> 
   <id>jboss</id> 
   <name>jboss</name> 
   <url>http://repository.jboss.org/maven2</url> 
  </repository> 
 </repositories>
</project>
